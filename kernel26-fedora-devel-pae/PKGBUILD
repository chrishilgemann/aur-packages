# Maintainer: Phillip Smith <fukawi2@NO-SPAM.gmail.com>
# http://github.com/fukawi2/aur-packages

pkgname="kernel26-fedora-devel-pae"
pkgver=2.6.35
_FEDORA_RELEASE=3.fc14
pkgrel=1
url="http://www.kernel.org/"
pkgdesc="development files for linux kernel and modules, compiled by Fedora with PAE supported. Release $_FEDORA_RELEASE"
arch=('i686')
license=('GPL2')
depends=('kernel26-fedora-pae')
makedepends=('rpmextract')
backup=('etc/mkinitcpio.d/kernel26.preset')
provides=("kernel26=$pkgver" "kernel26-fedora-pae=$pkgver")
conflicts=('kernel24' 'kernel24-scsi' 'kernel26-scsi' 'kernel26' 'kernel26-fedora')
# We need to force the upgrade because the previous maintainer
# used the Fedora release number as the pkgrel number so we could
# never have more than one release of each PKGBUILD!
options=(force)
source=("http://kojipkgs.fedoraproject.org/packages/kernel/${pkgver}/${_FEDORA_RELEASE}/i686/kernel-PAE-devel-${pkgver}-${_FEDORA_RELEASE}.i686.rpm")
md5sums=('9b0e7656041467ae2ae3f9463a314faf')

build() {
  msg "Nothing to compile for $pkgname"
}

package() {
  replaces=('kernel-devel-pae')
  
	cd $pkgdir
  
	rpmextract.sh $srcdir/kernel-PAE-devel-${pkgver}-${_FEDORA_RELEASE}.${CARCH}.rpm
}

# vim:set ts=2 sw=2 et:
